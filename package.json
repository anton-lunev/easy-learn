{
  "name": "easy-learn",
  "version": "0.0.1",
  "main": "./main.js",
  "publishName": "myApp",
  "electronVersion": "1.2.5",
  "scripts": {
    "devApp": "NODE_ENV=dev electron ./src/main.js",
    "devServer": "NODE_ENV=dev webpack-dev-server --hot --inline --progress --colors",
    "prod": "NODE_ENV=production webpack --bail --progress --profile --colors",
    "prodMac": "electron-packager ./dist/ easy-learn --platform=darwin --arch=x64 --out ~/Desktop/easy-learn --asar --version $npm_package_electronVersion --overwrite --icon=./src/images/app-icon.icns",
    "prodWin": "electron-packager ./dist/ easy-learn --platform=win32 --arch=x64 --out ~/Desktop/easy-learn --version $npm_package_electronVersion --overwrite --icon=./src/images/app-icon.ico"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.13.0",
    "browser-sync-spa": "^1.0.3",
    "clean-webpack-plugin": "^0.1.8",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.2.5",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "ng-annotate-loader": "^0.1.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.7.0",
    "postcss-csso": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.0",
    "svg-sprite-loader": "0.0.24",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-svgstore-plugin": "^3.0.0-beta"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-local-storage": "^0.2.7",
    "angular-ui-router": "^1.0.0-alpha.5",
    "lokijs": "^1.3.19"
  }
