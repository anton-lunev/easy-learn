{
  "name": "easy-learn",
  "version": "0.0.1",
  "main": "./main.js",
  "publishName": "myApp",
  "electronVersion": "0.36.9",
  "scripts": {
    "devApp": "NODE_ENV=dev electron ./src/main.js",
    "devServer": "NODE_ENV=dev webpack-dev-server --hot --inline --progress --colors",
    "prod": "NODE_ENV=production webpack --bail --progress --profile --colors",
    "prodMac": "electron-packager ./dist/ easy-learn --platform=darwin --arch=x64 --out ~/Desktop/easy-learn --asar --version $npm_package_electronVersion --overwrite --icon=./src/images/app-icon.icns",
    "prodWin": "electron-packager ./dist/ easy-learn --platform=win32 --arch=x64 --out ~/Desktop/easy-learn --version $npm_package_electronVersion --overwrite --icon=./src/images/app-icon.ico"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.11.1",
    "browser-sync-spa": "^1.0.3",
    "clean-webpack-plugin": "^0.1.8",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.9",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.10.0",
    "ng-annotate-loader": "^0.1.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.1",
    "postcss-csso": "^1.0.0",
    "postcss-loader": "^0.8.2",
    "style-loader": "^0.13.0",
    "svg-sprite-loader": "0.0.18",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-svgstore-plugin": "^2.1.3"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-local-storage": "^0.2.6",
    "angular-ui-router": "^0.2.18",
    "lokijs": "^1.3.16"
  }
