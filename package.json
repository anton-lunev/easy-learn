{
  "name": "easy-learn",
  "version": "0.1.0",
  "main": "./main.js",
  "publishName": "myApp",
  "electronVersion": "0.36.9",
  "scripts": {
    "start": "export NODE_ENV='dev' && electron ./src/main.js",
    "buildMac": "electron-packager ./dist/ easy-learn --platform=darwin --arch=x64 --out ~/Desktop/easy-learn --asar --version 0.36.9 --overwrite --icon=./src/images/app-icon.icns",
    "buildwin": "electron-packager ./dist/ easy-learn --platform=win32 --arch=x64 --out ~/Desktop/easy-learn --version 0.36.9 --overwrite --icon=./src/images/app-icon.ico",
    "devServer": "webpack-dev-server --hot --inline --progress --colors",
    "buildProd": "webpack --bail --progress --profile --colors"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.11.1",
    "browser-sync-spa": "^1.0.3",
    "clean-webpack-plugin": "^0.1.8",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.9",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.10.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "style-loader": "^0.13.0",
    "svg-sprite-loader": "0.0.18",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-svgstore-plugin": "^2.1.3"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-local-storage": "^0.2.6",
    "angular-ui-router": "^0.2.18",
    "lokijs": "^1.3.16"
  }
